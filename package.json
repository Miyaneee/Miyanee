  "homepage": "https://github.com/electron-react-boilerplate/electron-react-boilerplate#readme",
  "scripts": {
    "prepare": "husky install | cross-env ELECTRON_RUN_AS_NODE=1 npx --no-install electron scripts/updateVenders.cjs",
    "start": "node scripts/start.cjs",
    "build:renderer": "cd src/renderer && vite build",
    "build:preload": "cd src/preload && vite build",
    "build:main": "cd src/main && vite build",
    "build": "cross-env MODE=production npm run build:renderer && npm run build:main && npm run build:preload",
    "package": "rimraf dist && npm run build && electron-builder --config electron-builder.config.cjs",
    "lint": "eslint ./src/**/*.{ts,tsx} --max-warnings 0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "electron": "^19.0.9",
    "electron-builder": "^23.1.0",
