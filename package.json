    "less": "^4.1.3",
    "node-fetch": "^3.2.9",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.2"
  }
}
