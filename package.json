  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Miyaneee/miyanee.git"
  },
  "author": {
    "name": "Miyaneee",
    "email": "gutianhuang@foxmail.com",
    "url": "https://github.com/Miyaneee"
